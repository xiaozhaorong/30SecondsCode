{
    "plugins": [
        "back-to-top-button",
        "code",
        "splitter",
        "chapter-fold",
        "hide-element",
        "-lunr",
        "-search",
        "search-pro",
        "-sharing",
        "sharing-plus",
        "tbfed-pagefooter",
        "github",
        "edit-link",
        "klipse"
    ],
    "pluginsConfig": {
        "hide-element": {
            "elements": [
                ".gitbook-link"
            ]
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "pocket": false,
            "qq": false,
            "qzone": false,
            "twitter": false,
            "weibo": false,
            "all": []
        },
        "tbfed-pagefooter": {
            "copyright": "xiaozhaorong",
            "modify_label": "该文章修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "github": {
            "url": "https://github.com/xiaozhaorong"
        },"edit-link": {
            "base": "//github.com/xiaozhaorong/30SecondsCode/edit/main",
            "label": "编辑此页面"
        }
    }
}